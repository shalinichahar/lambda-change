{
  "name": "ssr",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:client && npm run build:server && npm install --only=production && rimraf ./node_modules/@sparticuz/chromium/bin",
    "start": "node dist/server.js",
    "dev": "nodemon --exec babel-node src/server/index.js",
    "build:client": "webpack --config webpack.client.js",
    "build:server": "webpack --config webpack.server.js",
    "build:css": "tailwindcss build src/shared/tailwind.css -o public/output.css",
    "package": "zip -r lambda-deployment.zip dist node_modules package.json",
    "deploy": "npm run build && npm run package"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
